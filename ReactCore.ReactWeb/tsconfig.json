{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es6",
      "scripthost",
      "es2015.iterable"
    ],
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "plugins": [ { "name": "typescript-plugin-css-modules" } ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [ "src" ],
  "files": [
    "app.tsx"
  ]
}